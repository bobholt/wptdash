# mysite_uwsgi.ini file
[uwsgi]

# process-related settings
socket          = /tmp/wptdash.sock
chdir           = /var/www/wptdash
master          = true
module          = wptdash.prodapp:prod_app
processes       = 5
logger          = file:/var/www/wptdash/logs/uwsgi.log
vacuum          = true
enable-threads  = true
die-on-term     = true
virtualenv      = /home/wptdash/venv
lazy						= true
lazy-apps				= true
